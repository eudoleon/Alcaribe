<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="product_normal_form_view" model="ir.ui.view">
            <field name="name">ProductNormalFormView</field>
            <field name="model">product.product</field>
            <field name="inherit_id" ref="product.product_normal_form_view"/>
            <field name="arch" type="xml">   
                <group name="group_standard_price" position="before">
                    <group name="group_fob_price">
                        <label for="fob_price" class="mt-1"/>
                        <div name="fobprice">
                            <field name="fob_price"/>
                        </div>
                        <label for="fob_percentage" class="mt-1"/>
                        <div name="fobpercentage">
                            <field name="fob_percentage"/>
                        </div>
                    </group>
                </group>
            </field>
        </record>

    </data>
</odoo>
